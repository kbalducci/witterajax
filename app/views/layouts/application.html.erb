<!DOCTYPE html>
<html>
<head>
  <title>Witterajax</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="container">

    <div class="row">
      <header class='banner'>
        <h1> <%= link_to "Witter", root_path %></h1>


      </header>
    </div>

    <div class="row">
      <div class="content">
         <%= yield %>
      </div>
    </div>

    <div class="row">
      <footer>
        <p>Made with love</p>
      </footer>
    </div>

</body>
</html>
